<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatioImageView">
        <attr name="riv_ration" format="float" />
        <attr name="riv_orientation" format="enum">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RatioRelativeLayout">
        <attr name="rrl_ration" format="float" />
        <attr name="rrl_orientation" format="enum">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
        </attr>
    </declare-styleable>

</resources>